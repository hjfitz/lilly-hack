<div class=center>
<h1>Welcome!</h1>
<p>Welcome to {{title}}.</p>
<p>Do you already have an account?</p>
<a class="waves-effect waves-light btn light-green" id="btnYes"><i class="material-icons medium right">done</i>Yes</a>
<a class="waves-effect waves-light btn red lighten-1" id="btnNo"><i class="material-icons medium right">close</i>No</a>

<!-- hidden stuff for the create account form -->
<div class="row hidden" id="loginForm">
  <form class="col s12">
    <div class="row">
      <div class="input-field col m6 s3">
        <input id="user_name" type="text" class="validate">
        <label for="user_name">Userame</label>
      </div>
      <div class="input-field col m6 s3">
        <input id="pass" type="password" class="validate">
        <label for="pass">Password</label>
      </div>
    </div>
  </form>
<a class="waves-effect waves-light btn" id="btnSub">Submit</a>
</div>
</div>
<div id=errorbox class="card hidden" >
  <div class="col s12 m6">
    <div id="statCard" class="card red darken-3">
     <div class="card-content white-text">
       <span id="statTitle" class="card-title">Error</span>
       <p id=error>error text goes here</p>
       <p id=errorDebug></p>
     </div>
        <div class="card-action">
         <a href="#" id="errorHide">I understand</a>
        </div>
      </div>
    </div>
</div>

</div>
<script src="https://cdnjs.cloudflare.com/ajax/libs/blueimp-md5/2.5.0/js/md5.min.js"></script>
<script src="/javascripts/main.js"></script>
